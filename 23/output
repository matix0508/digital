#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55e0d62da0d0 .scope module, "tb_shift_reg" "tb_shift_reg" 2 1;
 .timescale 0 0;
v0x55e0d6324c00_0 .var "clk", 0 0;
v0x55e0d6324cc0_0 .var "data", 0 0;
v0x55e0d6324d90_0 .var "en", 0 0;
v0x55e0d6324e90_0 .net "out", 4 0, v0x55e0d6324970_0;  1 drivers
v0x55e0d6324f60_0 .var "rstn", 0 0;
S_0x55e0d62da250 .scope module, "sr0" "shift_reg" 2 9, 3 1 0, S_0x55e0d62da0d0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "d"
    .port_info 1 /INPUT 1 "clk"
    .port_info 2 /INPUT 1 "en"
    .port_info 3 /INPUT 1 "rstn"
    .port_info 4 /OUTPUT 5 "out"
P_0x55e0d6312170 .param/l "MSB" 0 3 1, +C4<00000000000000000000000000000101>;
v0x55e0d62ee7d0_0 .net "clk", 0 0, v0x55e0d6324c00_0;  1 drivers
v0x55e0d62eea10_0 .net "d", 0 0, v0x55e0d6324cc0_0;  1 drivers
v0x55e0d63248d0_0 .net "en", 0 0, v0x55e0d6324d90_0;  1 drivers
v0x55e0d6324970_0 .var "out", 4 0;
v0x55e0d6324a50_0 .net "rstn", 0 0, v0x55e0d6324f60_0;  1 drivers
E_0x55e0d63127e0 .event posedge, v0x55e0d62ee7d0_0;
    .scope S_0x55e0d62da250;
T_0 ;
    %wait E_0x55e0d63127e0;
    %load/vec4 v0x55e0d6324a50_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v0x55e0d6324970_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55e0d63248d0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x55e0d6324970_0;
    %assign/vec4 v0x55e0d6324970_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x55e0d62eea10_0;
    %load/vec4 v0x55e0d6324970_0;
    %parti/s 4, 1, 2;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x55e0d6324970_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55e0d62da0d0;
T_1 ;
    %delay 10, 0;
    %load/vec4 v0x55e0d6324c00_0;
    %inv;
    %store/vec4 v0x55e0d6324c00_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55e0d62da0d0;
T_2 ;
    %vpi_call 2 20 "$dumpfile", "shift_register_zad23_tb.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55e0d62da0d0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55e0d6324c00_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55e0d6324d90_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55e0d6324f60_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55e0d6324cc0_0, 0;
    %end;
    .thread T_2;
    .scope S_0x55e0d62da0d0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55e0d6324f60_0, 0;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55e0d6324f60_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55e0d6324d90_0, 0;
    %pushi/vec4 7, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x55e0d63127e0;
    %load/vec4 v0x55e0d6324cc0_0;
    %inv;
    %assign/vec4 v0x55e0d6324cc0_0, 0;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %pushi/vec4 7, 0, 32;
T_3.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.3, 5;
    %jmp/1 T_3.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x55e0d63127e0;
    %jmp T_3.2;
T_3.3 ;
    %pop/vec4 1;
    %vpi_call 2 38 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x55e0d62da0d0;
T_4 ;
    %vpi_call 2 42 "$monitor", "rstn=%0b data=%b, en=%0b, out=%b", v0x55e0d6324f60_0, v0x55e0d6324cc0_0, v0x55e0d6324d90_0, v0x55e0d6324e90_0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
