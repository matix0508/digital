#! /usr/bin/vvp
:ivl_version "10.2 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1358ab0 .scope module, "test" "test" 2 1;
 .timescale 0 0;
v0x136b380_0 .var "clk", 0 0;
v0x136b450_0 .net "cnt", 3 0, v0x136b090_0;  1 drivers
v0x136b520_0 .var "rst_n", 0 0;
S_0x1358c30 .scope module, "c0" "counter" 2 9, 3 1 0, S_0x1358ab0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "rst_n"
    .port_info 2 /OUTPUT 4 "out"
v0x1356670_0 .net "clk", 0 0, v0x136b380_0;  1 drivers
v0x136b090_0 .var "out", 3 0;
v0x136b170_0 .net "rst_n", 0 0, v0x136b520_0;  1 drivers
v0x136b240_0 .var "x", 0 0;
E_0x1356f60/0 .event negedge, v0x136b170_0;
E_0x1356f60/1 .event posedge, v0x1356670_0;
E_0x1356f60 .event/or E_0x1356f60/0, E_0x1356f60/1;
    .scope S_0x1358c30;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x136b240_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x1358c30;
T_1 ;
    %wait E_0x1356f60;
    %load/vec4 v0x136b170_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x136b090_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x136b090_0;
    %parti/s 1, 3, 3;
    %load/vec4 v0x136b090_0;
    %parti/s 1, 2, 3;
    %xnor;
    %assign/vec4 v0x136b240_0, 0;
    %load/vec4 v0x136b090_0;
    %parti/s 3, 0, 2;
    %load/vec4 v0x136b240_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x136b090_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x1358ab0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x136b380_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x136b520_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x1358ab0;
T_3 ;
    %delay 1, 0;
    %load/vec4 v0x136b380_0;
    %inv;
    %store/vec4 v0x136b380_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x1358ab0;
T_4 ;
    %vpi_call 2 11 "$dumpfile", "counter.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x1358ab0 {0 0 0};
    %delay 15, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x136b520_0, 0, 1;
    %delay 72, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x1358ab0;
T_5 ;
    %vpi_call 2 19 "$monitor", "At time %t, value = %d (%b)", $time, v0x136b450_0, v0x136b450_0 {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter.tb.v";
    "counter.v";
